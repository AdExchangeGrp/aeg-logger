{
  "name": "@adexchange/aeg-logger",
  "version": "0.0.26",
  "description": "Configurable logger",
  "main": "./lib/logger.js",
  "author": "Justin Durham",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdExchangeGrp/aeg-logger.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "start": "node index.js",
    "test": "mocha --recursive --compilers js:babel-core/register test/",
    "test:coverage": "istanbul cover _mocha -- --recursive --compilers js:babel-core/register test/",
    "jshint": "jshint .",
    "jshint:watch": "npm run jshint && onchange \"**/*.js\" \".jshint*\" -- npm run jshint",
    "dev": "parallelshell \"npm run jshint:watch\"",
    "build": "npm run jshint && mocha --recursive --reporter mocha-junit-reporter --compilers js:babel-core/register test/unit/",
    "compile": "babel -d ./lib/ ./src/",
    "patch": "git reset --hard HEAD && git clean -fd && npm run compile && npm version patch -m \"[ci skip] %s\" && git push origin master && git push origin master --tags && npm publish"
  },
  "devDependencies": {
    "istanbul": "^0.3.21",
    "mocha": "^2.3.3",
    "mocha-junit-reporter": "^1.9.1",
    "onchange": "^2.0.0",
    "parallelshell": "^2.0.0",
    "should": "^7.1.0"
  },
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "config": "^1.16.0",
    "lodash": "^3.10.1",
    "winston": "^1.0.2",
    "winston-error": "^0.1.0",
    "winston-papertrail": "^1.0.1"
  },
  "engine": "node >= 4.1.0"
}
