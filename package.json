{
  "name": "@adexchange/aeg-logger",
  "version": "0.0.32",
  "description": "Configurable logger",
  "main": "./lib/logger.js",
  "author": "Justin Durham",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdExchangeGrp/aeg-logger.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "start": "node index.js",
    "test": "mocha --recursive --compilers js:babel-core/register test/",
    "test:coverage": "istanbul cover _mocha -- --recursive --compilers js:babel-core/register test/",
    "eslint": "eslint .",
    "eslint:watch": "npm run eslint && onchange \"**/*.js\" \".eslint*\" -- npm run eslint",
    "compile": "babel -d ./lib/ ./src/",
    "build": "npm run eslint && npm run compile && istanbul cover _mocha -- --recursive --compilers js:babel-core/register --reporter mocha-junit-reporter test/unit",
    "patch": "git reset --hard HEAD && git clean -fd && npm run compile && npm version patch -m \"[ci skip] %s\" && git push origin master && git push origin master --tags && npm publish"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "eslint": "^3.5.0",
    "eslint-config-standard": "^6.0.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "istanbul": "^0.3.21",
    "mocha": "^2.3.3",
    "mocha-junit-reporter": "^1.9.1",
    "onchange": "^2.0.0",
    "should": "^7.1.0"
  },
  "dependencies": {
    "config": "^1.21.0",
    "js-yaml": "^3.6.1",
    "lodash": "^4.15.0",
    "winston": "^2.2.0",
    "winston-error": "^1.0.1",
    "winston-papertrail": "^1.0.3"
  },
  "engine": "node >= 4.1.0"
}
