{
  "name": "@adexchangedevops/aeg-logger",
  "version": "0.0.19",
  "description": "Configurable logger",
  "main": "./lib/logger.js",
  "author": "Justin Durham",
  "repository": {
    "type": "git",
    "url": "git://git@bitbucket.org:AdExchangeGrp/aeg-logger.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "test": "mocha --recursive test/",
    "test:watch": "npm run test -- -w",
    "test:coverage": "istanbul cover _mocha -- --recursive test/",
    "jshint": "jshint .",
    "jshint:watch": "npm run jshint && onchange \"**/*.js\" \".jshint*\" -- npm run jshint",
    "dev": "parallelshell \"npm run jshint:watch\" \"npm run test:watch\"",
    "build": "npm run jshint && mocha --recursive test/ -R xunit-file",
    "postversion": "git push && git push --tags && npm publish ."
  },
  "devDependencies": {
    "should": "^7.1.0",
    "mocha": "^2.3.3",
    "parallelshell": "^2.0.0",
    "onchange": "^2.0.0",
    "xunit-file": "0.0.7",
    "istanbul": "^0.3.21"
  },
  "dependencies": {
    "config": "^1.16.0",
    "lodash": "^3.10.1",
    "winston": "^1.0.2",
    "winston-error": "^0.1.0",
    "winston-papertrail": "^1.0.1"
  },
  "engine": "node >= 4.1.0"
}
